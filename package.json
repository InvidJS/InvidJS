{
  "name": "@invidjs/invid-js",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "tsup": {
    "target": "node16"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "version": "1.0.2",
  "description": "An attempt to add Invidious support to Node.js",
  "scripts": {
    "prettier": "npx prettier index.ts ./api/* ./tests/* ./utils/* --write",
    "test": "jest ./tests/ --verbose",
    "build": "tsup index.ts --format cjs,esm --dts --clean && cp package.json dist && cp README.md dist",
    "docs:build": "npx typedoc --plugin typedoc-plugin-missing-exports --name InvidJS --cleanOutputDir false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/InvidJS/InvidJS.git"
  },
  "author": "alexavil",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "invidious",
    "invidious-api"
  ],
  "bugs": {
    "url": "https://github.com/InvidJS/InvidJS/issues"
  },
  "homepage": "https://invidjs.github.io/docs/",
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "axios": "^1.3.6",
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tsup": "^6.6.3",
    "typedoc": "^0.24.6",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.4"
  }
}

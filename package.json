{
  "name": "@alexavil/invid-js",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "tsup": {
    "target": "node16"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/alexavil"
  },
  "version": "0.0.3",
  "description": "An attempt to add Invidious support to Node.js",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean && copy package.json dist",
    "docs:build": "documentation build ./index.ts -f html -o docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexavil/InvidJS.git"
  },
  "author": "alexavil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexavil/InvidJS/issues"
  },
  "homepage": "https://github.com/alexavil/InvidJS#readme",
  "dependencies": {
    "got": "^12.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
